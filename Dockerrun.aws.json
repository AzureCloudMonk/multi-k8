{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "stephengrider/multi-nginx",
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["frontend", "backend"]
    },
    {
      "name": "frontend",
      "image": "stephengrider/multi-client",
      "hostname": "frontend",
      "memory": 128
    },
    {
      "name": "backend",
      "image": "stephengrider/multi-server",
      "hostname": "backend",
      "memory": 128
    },
    {
      "name": "worker",
      "image": "stephengrider/multi-worker",
      "memory": 128
    }
  ]
}
