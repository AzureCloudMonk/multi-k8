{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "nginx",
      "image": "stephengrider/multi-nginx",
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["frontend"],
      "essential": false
    },
    {
      "name": "frontend",
      "image": "stephengrider/multi-client",
      "hostname": "frontend",
      "memory": 256,
      "essential": true,
      "links": ["nginx"]
    }
  ]
}
